{
	"name": "myvscodetool",
	"displayName": "myvscodetool",
	"description": "",
	"version": "0.0.1",
	"publisher":"cxx1357220",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:myvscodetool.clearLog",
		"onCommand:myvscodetool.log",
		"onCommand:myvscodetool.china",
		"onCommand:myvscodetool.notesLog",
		"onCommand:myvscodetool.fanyi"		
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "myvscodetool.clearLog",
				"title": "clear log"
			},
			{
				"command": "myvscodetool.log",
				"title": "my log"
			},
			{
				"command": "myvscodetool.china",
				"title": "China"
			},
			{
				"command": "myvscodetool.notesLog",
				"title": "notes log"
			}
		],
		"keybindings": [
			{
				"command": "myvscodetool.log",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus"
			},
			{
				"command": "myvscodetool.fanyi",
				"key": "ctrl+shift+f",
				"mac": "cmd+shift+f",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"axios": "^0.21.1"
	}
}
